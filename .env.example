# Exemplo de arquivo .env para desenvolvimento local
# Copie para .env e customize conforme necessário

# ============================================
# MODO DESENVOLVIMENTO LOCAL (MinIO + Local)
# ============================================

# Usar arquivo local (não requer S3/MinIO)
STORAGE_MODE=local
LOCAL_BRONZE_PATH=./pipeline/data/bronze
LOCAL_SILVER_PATH=./pipeline/data/silver
LOCAL_GOLD_PATH=./pipeline/data/gold

# OU usar MinIO (Docker local)
# STORAGE_MODE=minio
# AWS_ENDPOINT_URL=http://localhost:9000
# AWS_ACCESS_KEY_ID=minioadmin
# AWS_SECRET_ACCESS_KEY=minioadmin123

# OU usar AWS S3 real
# STORAGE_MODE=s3
# AWS_ACCESS_KEY_ID=seu-access-key
# AWS_SECRET_ACCESS_KEY=seu-secret-key
# AWS_S3_REGION_NAME=us-east-1

# ============================================
# BUCKETS / STORAGE
# ============================================
AWS_S3_BRONZE_BUCKET=bronze
AWS_S3_SILVER_BUCKET=silver
AWS_S3_GOLD_BUCKET=gold

# ============================================
# POSTGRESQL / POSTGIS
# ============================================
RDS_HOST=localhost
# RDS_HOST=postgis     # Se usar Docker
RDS_PORT=5432
RDS_NAME=esteira_geo
RDS_USER=esteira_user
RDS_PASSWORD=esteira_local_2025

# ============================================
# LOGGING
# ============================================
LOG_LEVEL=INFO
LOG_FILE=./logs/pipeline.log

# ============================================
# FLASK
# ============================================
FLASK_ENV=development
FLASK_DEBUG=1
